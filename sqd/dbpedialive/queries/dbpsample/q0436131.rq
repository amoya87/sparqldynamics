
SELECT * 
WHERE 
{ <http://dbpedia.org/resource/Barfruesh>
            <http://www.w3.org/2000/01/rdf-schema#label>  ?label
  OPTIONAL
    { <http://dbpedia.org/resource/Barfruesh>
                <http://www.w3.org/2003/01/geo/wgs84_pos#lat>  ?lat ;
                <http://www.w3.org/2003/01/geo/wgs84_pos#long>  ?long
    }
}
